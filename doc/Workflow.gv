digraph G {
	ranksep=0.3;
	node [ fontname=Helvetica ];
	src [ label="tcas.c" ];
	prep [ label="tcas.i.c" ];
	trans [ label="tcas.i.trans.c" ];
	obj [ label="tcas.i.trans.o" ];
	dbginfo [ label="tcas.i.trans.c.dbginfo" ];
	rt_h [ label="instcov_rt.h" ];
