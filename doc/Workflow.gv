	mcdc_report [ label="MC/DC Coverage Report" ];
	node [ shape=box ];
	p_prep [ label="gcc -E (preprocessing)" ];
	p_instcov [ label="instcov (instrumentation)" ];
	p_compile [ label="gcc -c (compilation)" ];
	p_link [ label="ld (linking)" ];
